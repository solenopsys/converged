{
  "name": "back-core",
  "version": "1.0.0",
  "module": "src/index.ts",
  "type": "module",
  "private": true,
  "scripts": {
    "dev": "bun run --hot --env-file=../../../conf/back-server.env src/services.ts",
    "start": "bun run src/services.ts",
    "typecheck": "bun tsc --noEmit",
    "lint": "biome check src",
    "fmt": "biome format --write src"
  },
  "dependencies": {
    "@elysiajs/cors": "latest",
    "@elysiajs/jwt": "latest",
    "bcryptjs": "latest",
    "elysia": "latest",
    "kysely": "latest",
    "lmdb": "latest",
    "lowdb": "^7.0.1",
    "pg": "latest"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.9.4",
    "@types/bcryptjs": "latest",
    "@types/bun": "latest",
    "@types/pg": "latest",
    "@typescript-eslint/typescript-estree": "^8.45.0",
    "typescript": "^5.9.3"
  },
  "engines": {
    "bun": ">=1.0.0"
  }
}